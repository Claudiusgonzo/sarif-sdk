              "shortDescription": {
                "text": "Each result location must provide the property 'physicalLocation.artifactLocation.uri'."
              },
              "fullDescription": {
                "text": "Each result location must provide the property 'physicalLocation.artifactLocation.uri'. The GitHub Developer Security Portal will not display a result whose location does not provide the URI of the artifact that contains the result."
              },
              "messageStrings": {
                "LocationsMustHaveRequired_Properties_Error_NoLocationsArray_Text": {
                  "text": "{0}: The 'locations' property is absent. The GitHub Developer Security Portal will not display a result unless it provides a location that specifies the URI of the artifact that contains the result."
                },
                "LocationsMustHaveRequired_Properties_Error_EmptyLocationsArray_Text": {
                  "text": "{0}: The 'locations' array is empty. The GitHub Developer Security Portal will not display a result unless it provides a location that specifies the URI of the artifact that contains the result."
